{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}",
      "pythonArgs": [
        "-Xfrozen_modules=off"
      ],
      "env": {
        "PYDEVD_DISABLE_FILE_VALIDATION": "1"
      }
      // "args": ["-i", "response.xml", "-o", "response.csv"],
      // "args": ["-d", "/Volumes/Data"],
      // "args": ["-f", "menubar.dmg"],
      // "args": ["-h"],
    },
    {
      "type": "rdbg",
      "name": "Debug current file with rdbg",
      "request": "launch",
      "script": "${file}",
      "cwd": "${workspaceFolder}",
      "args": [],
      "askParameters": true
    },
    {
      "type": "rdbg",
      "name": "Attach with rdbg",
      "request": "attach"
    }
  ]
}
